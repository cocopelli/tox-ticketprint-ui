{
  "name": "tox-ticketprint-ui",
  "version": "0.0.2",
  "description": "AngularJS frontend app for TicketPrint of MovingImage24",
  "main": "serve.js",
  "author": "Enrico Hoffmann <dasrick@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dasrick/tox-ticketprint-ui.git"
  },
  "bugs": {
    "url": "https://github.com/dasrick/tox-ticketprint-ui/issues"
  },
  "homepage": "https://github.com/dasrick/tox-ticketprint-ui#readme",
  "devDependencies": {
    "angular": "^1.4.7",
    "angular-mocks": "^1.4.7",
    "david": "^6.4.0",
    "express": "^4.13.3",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.4",
    "jscs": "^2.3.5",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "json-loader": "^0.5.3",
    "karma": "^0.13.11",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "0.0.10",
    "phantomjs": "^1.9.18",
    "webpack": "^1.12.2"
  },
  "scripts": {
    "postinstall": "npm run build",
    "prebuild": "mkdir -p web/js/",
    "build": "npm run jscs && npm run jshint && npm run build:js",
    "build:js": "webpack --config webpack.config.js",
    "jscs": "jscs ./src ./tests",
    "jshint": "jshint ./src ./tests --reporter node_modules/jshint-stylish/index.js",
    "start": "node serve.js",
    "test": "./node_modules/karma/bin/karma start karma.config.js"
  }
}
